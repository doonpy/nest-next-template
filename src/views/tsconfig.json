{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": false,
    "typeRoots": [
      "../@types",
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "../@types"
  ]
}
