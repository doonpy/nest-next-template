{
  "name": "nest-next-template",
  "version": "0.0.1",
  "description": "Nest + Next template",
  "author": "Poon Nguyen",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "sh scripts/ci/build.sh",
    "start-api:prod": "node dist/apps/api/main",
    "start-api:dev": "nest start --watch",
    "prisma-api:studio": "prisma studio",
    "prisma-api:generate": "prisma generate",
    "start-web:dev": "next dev apps/web -p 3001",
    "lint": "eslint --fix --max-warnings=0 --ext .ts,.tsx apps",
    "type-check": "tsc -p ./apps/web/tsconfig.json & tsc -p ./apps/api/tsconfig.app.json",
    "type-check:dev": "tsc -w"
  },
  "engines": {
    "node": "^14.15.x",
    "yarn": "^1.22.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.10",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.2",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/platform-express": "^7.5.1",
    "@prisma/client": "2.19.0",
    "apollo-server-express": "^2.21.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.3",
    "joi": "^17.3.0",
    "morgan": "^1.10.0",
    "next": "^10.0.5",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.2.7",
    "@svgr/webpack": "^5.5.0",
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel": "^6.23.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "preact": "^10.5.13",
    "prettier": "^2.1.2",
    "prisma": "2.19.0",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
